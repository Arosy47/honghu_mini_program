
page{
  background-color: white;
}

.sell_cells {

  background-color: rgb(255, 255, 255);
}

.sell_cell {

  display: flex;
  padding: 0rpx 30rpx;
  align-items: center;
  justify-content: space-between;
  background-color: rgb(255, 255, 255);
  height: 90rpx;

}

.line{
  width: 100%;
  border-bottom: 1rpx solid #000;
  -webkit-transform: scaleY(0.1);
  transform: scaleY(0.1); 
}

.sell_title {
  
  width: auto;
  font-size: 15px;
  
}

.right-jiantou{
  right: 10rpx;
  margin-left: 10rpx;
  text-align:right;
  font-weight:normal;
  font-size: 23rpx;
  
  color: #999999;
}

.color-gray{
  color: #cccccc;
}

.highSelectColor{
  
  background-color: rgba(0, 0, 0, 0.05);
}